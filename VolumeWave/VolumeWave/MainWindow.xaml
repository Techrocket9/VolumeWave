<Window x:Name="Audio_Cycle" x:Class="VolumeWave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Audio Cycle" Height="260" Width="525" Closed="Audio_Cycle_Closed">
    <Grid>
        <Slider x:Name="FrequencyAdj" HorizontalAlignment="Left" Margin="161,127,0,0" VerticalAlignment="Top" Height="71" Width="163" ValueChanged="setnewperiod"/>
        <Button Content="Drop 10 ms" HorizontalAlignment="Left" Margin="202,198,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Faster" HorizontalAlignment="Left" Margin="344,127,0,0" VerticalAlignment="Top"/>
        <Label Content="Slower" HorizontalAlignment="Left" Margin="101,127,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
